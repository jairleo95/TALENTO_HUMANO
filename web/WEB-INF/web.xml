<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 
      <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>pe.edu.upeu.rrhh.controlador.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Proveedor</servlet-name>
        <servlet-class>pe.edu.upeu.rrhh.controlador.Proveedor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CIndex</servlet-name>
        <servlet-class>pe.edu.upeu.application.web.controller.CIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dota</servlet-name>
        <servlet-class>pe.edu.upeu.rrhh.controlador.Dota</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Proveedor</servlet-name>
        <url-pattern>/proveedor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CIndex</servlet-name>
        <url-pattern>/cindex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dota</servlet-name>
        <url-pattern>/dota</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
  
</web-app>
